# Kenya Overwatch Environment Variables
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/kenya_overwatch

# Authentication
SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# API
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,https://kenya-overwatch.go.ke

# AI Pipeline
AI_MODEL_PATH=/models
AI_CONFIDENCE_THRESHOLD=0.7

# WebSocket
WS_HOST=0.0.0.0
WS_PORT=8001

# Logging
LOG_LEVEL=INFO
LOG_FILE=/var/log/kenya-overwatch/api.log

# Redis (for caching)
REDIS_URL=redis://localhost:6379/0

# Security
RATE_LIMIT_PER_MINUTE=60
SESSION_SECRET=your-session-secret
